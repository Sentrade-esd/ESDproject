{
  "rabbit_version": "3.13.0",
  "users": [
    {
      "name": "guest",
      "password": "guest",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "guest",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "exchanges": [
    {
      "name": "notifications_exchange",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "comments_exchange",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "stoploss_retry_exchange",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "stoploss_waiting_exchange",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "queues": [
    {
      "name": "sentiment_notification_queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "new_comment_queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "stoploss_retry_queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "stoploss_waiting_queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-dead-letter-exchange": "stoploss_retry_exchange",
        "x-message-ttl": 10000
      }
    }
  ],
  "bindings": [
    {
      "source":"notifications_exchange", 
      "vhost":"/", 
      "destination":"sentiment_notification_queue", 
      "destination_type":"queue", 
      "routing_key":"notify",
      "arguments": {}
    },
    {
      "source":"comments_exchange", 
      "vhost":"/", 
      "destination":"new_comment_queue", 
      "destination_type":"queue", 
      "routing_key":"comment",
      "arguments": {}
    },
    {
      "source":"stoploss_retry_exchange", 
      "vhost":"/", 
      "destination":"stoploss_retry_queue", 
      "destination_type":"queue", 
      "routing_key":"stoploss",
      "arguments": {}
    },
    {
      "source":"stoploss_waiting_exchange", 
      "vhost":"/", 
      "destination":"stoploss_waiting_queue", 
      "destination_type":"queue", 
      "routing_key":"stoploss",
      "arguments": {}
    }
  ]
}